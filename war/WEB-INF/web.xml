<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
      version="3.0"> 
	<servlet>
		<servlet-name>Computacion</servlet-name>
		<servlet-class>youtube.ComputacionServlet</servlet-class>
	</servlet>
		
		<servlet>
		<servlet-name>Upload</servlet-name>
		<servlet-class>youtube.Upload</servlet-class>
	</servlet>
	
			<servlet>
		<servlet-name>Delete</servlet-name>
		<servlet-class>youtube.Delete</servlet-class>
	</servlet>
	
			<servlet>
		<servlet-name>MyVideos</servlet-name>
		<servlet-class>youtube.MyVideos</servlet-class>
	</servlet>
				<servlet>
		<servlet-name>CreatePlayList</servlet-name>
		<servlet-class>youtube.CreatePlayList</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Computacion</servlet-name>
		<url-pattern>/Computacion</url-pattern>
	</servlet-mapping>
	
	
		<servlet-mapping>
		<servlet-name>Upload</servlet-name>
		<url-pattern>/Upload</url-pattern>
	</servlet-mapping>
	
		
		<servlet-mapping>
		<servlet-name>Delete</servlet-name>
		<url-pattern>/Delete</url-pattern>
	</servlet-mapping>
	
		<servlet-mapping>
		<servlet-name>MyVideos</servlet-name>
		<url-pattern>/MyVideos</url-pattern>
	</servlet-mapping>
			<servlet-mapping>
		<servlet-name>CreatePlayList</servlet-name>
		<url-pattern>/CreatePlayList</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
